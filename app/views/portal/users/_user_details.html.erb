

<%=image_tag(@user.user_detail.photo, size:'100x100')%>
<table>
	<thead>
		<th> Field</th>
		<th> Value</th>
	</thead>

	<tbody>	
		<tr>	
			<td>Email</td>
			<td><%= @user.email %></td>
		</tr>
		<tr>
			<td> First Name </td>
			<td> <%= @user.user_detail.try(:first_name) %> </td>
		</tr>
		<tr>
			<td> Last Name </td>
			<td> <%= @user.user_detail.try(:last_name) %></td>
		</tr>
		<tr>
			<td> Date Of Birth </td>
			<td> <%= @user.user_detail.try(:dob) %> </td>
		</tr>
		<tr>
			<td> Address </td>
			<td> <%= @user.user_detail.try(:address) %></td>
		</tr>
		<tr>	
			<td>Pincode</td>
			<td><%= @user.user_detail.try(:pincode) %></td>
		</tr>
		<tr>
			<td> State </td>
			<td> <%= @user.user_detail.try(:state) %></td>
		</tr>
		<tr>
			<td> Country </td>
			<td> <%= @user.user_detail.try(:country) %></td>
		</tr>
		<tr>
			<td> Mobile Number </td>
			<td> <%= @user.user_detail.try(:mobile) %></td>
		</tr>
		<tr>
			<td> Land Line</td>
			<td> <%= @user.user_detail.try(:landline)  %></td>
		</tr>
		<tr>
			<td> Institution </td>
			<td> <%= @user.institution.try(:name) %></td>
		</tr>
		<tr>
			<td> School  </td>
			<td> <%= @user.school.try(:name) %></td>
		</tr>
		
	</tbody>
</table>
