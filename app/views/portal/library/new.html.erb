<div>
  <%= form_for @library, :url => {:action => "create"}, :html => { :multipart => true } do |f| -%>   

    <%= render :partial => 'form', :locals => { :f => f } %>
    
   <%= f.submit "Submit", :disable_with => 'Submiting...',class:"btn btn-primary" %>
  <%end%> 
    <%= link_to "Cancel", portal_library_index_path, class:'btn btn-primary' %>
</div>