
<%= link_to "Schedule Exam", new_portal_exam_schedule_path, class:"btn btn-primary" %>
<br><br>
<%= text_field_tag :date,"", class:"date_picker" %>
<div id = "index_content">
  <table cellpadding="0" cellspacing="0" border="0" class="table table-bordered">
    <thead>
      <th> Exam Date</th>
      <th> Type</th>
      <th> School</th>
      <th> Question Paper</th>
      <th> Subject</th>
      <th> Action </th>
    </thead>
    <tbody>
      <% @exam_schedules.each do |es| %>
        <tr>
          <td><%= es.exam_date %></td>
          <td><%= es.type %></td>
          <td><%= es.school.name %> </td>
          <td> <%= es.question_paper.name %> </td>
          <td> <%= es.subject.name %> </td>
          <td><%= link_to "show", portal_exam_schedule_path(es) %> |  
          <%= link_to "Edit", edit_portal_exam_schedule_path(es) %> | 
          <%= link_to "Delete", portal_exam_schedule_path(es), method:'delete' %></td>
        </tr>
      <%end%>      
    </tbody>  
  </table>
</div>

<div id = "filtered">
  
</div>