<%= form_for @question_set, url:{action:'create'}, method:'post' do |f| -%>

	<%= f.label :name, "Name" %>
	<%= f.text_field :name %>
	<br>
	<%= f.submit "Create", :disable_with => 'Creating...', class:"btn btn-primary" %>
	<%= link_to "Cancel", portal_question_sets_path, class:'btn btn-inverse' %>	  
<% end %>