<%= form_for @question_set, url:{action:'update'}, method:'put' do |f| -%>

	<%= f.label :name, "Name" %>
	<%= f.text_field :name %>
	<br>
	<%= f.submit "Update", :disable_with => 'Upating...', class:"btn btn-primary" %>
	<%= link_to "Cancel", portal_question_sets_path, class:"btn btn-inverse" %>    
<% end %>